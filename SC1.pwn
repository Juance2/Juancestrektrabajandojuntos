	if (PlayerData[playerid][pIncomingCall])
	{
	    SendClientMessage(playerid, COLOR_YELLOW, "[Teléfono]:{FFFFFF} Has rechazado la llamada entrante.");
	    SendClientMessage(targetid, COLOR_YELLOW, "[Teléfono]:{FFFFFF} La otra linea ha rechazado la llamada.");

		SendNearbyMessage(playerid, 30.0, COLOR_PURPLE, "* %s has declined the call.", ReturnName(playerid, 0));
