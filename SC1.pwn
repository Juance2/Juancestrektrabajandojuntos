	        SendClientMessage(playerid, COLOR_LIGHTRED, "[ADVERTENCIA]:{FFFFFF} Estás a punto de abandonar tu vehículo sin posibilidad de reembolso.");
		}
		else if (CarData[id][carImpounded] != -1)
