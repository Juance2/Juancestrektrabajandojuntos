CMD:entregarcoche(playerid, params[])
{
	new vehicleid = GetPlayerVehicleID(playerid);
	new id = VehiculoRenta[playerid];

	if(TiempoRenta[playerid] == -1)
	    return SendErrorMessage(playerid, "No has rentado un vehículo aún.");

	if(!IsPlayerInVehicle(playerid, id))
	    return SendErrorMessage(playerid, "No estás en el vehículo rentado.");

	if (!IsPlayerInRangeOfPoint(playerid, 30.0, 1914.8663, -1862.9969, 13.5618))
	    return SendErrorMessage(playerid, "No estás en el punto de entrega.");

	SendClientMessage(playerid,-1,""CVERDE"[Car Max] Gracias por usar nuestro servicio de renta de vehículos, coche entregado.");
	Car_Delete(Car_GetID(vehicleid));
	VehiculoRenta[playerid] = -1;
 	TiempoRenta[playerid] = -1;

	return 1;
}

