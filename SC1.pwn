	if (strlen(params) > 64) {
	    foreach (new i : Player) if (!PlayerData[i][pDisableBC]) {
		    SendClientMessageEx(i, COLOR_LIGHTGREEN, "[Canal Estatal] Reportero %s: %.64s", ReturnName(playerid, 0), params);
