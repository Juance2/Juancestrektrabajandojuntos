CMD:spawnpoint(playerid, params[])
{
	new point;
	if(sscanf(params, "i", point)) return SendErrorMessage(playerid, "/spawnpoint [0-2] (0 = Aeropuerto, 1 = Facción, 2 = última conexión)");
	if(point == 0)
	{
	    SendClientMessage(playerid, COLOR_WHITE, "Has cambiado el spawn del aeropuerto.");
		PlayerData[playerid][pSpawnPoint] = 0;
		return 1;
	}
	if(point == 1)
	{
	    if(PlayerData[playerid][pFactionID] == -1)
	    {
	        SendErrorMessage(playerid, "No eres parte de una facción.");
	        return 1;
		}
		SendClientMessage(playerid, COLOR_WHITE, "Has cambiado el spawn a la de tu facción.");
		PlayerData[playerid][pSpawnPoint] = 1;
		return 1;
	}
	if(point == 2)
	{
	    SendClientMessage(playerid, COLOR_WHITE, "Has cambiado el spawn a tu última conexión.");
		PlayerData[playerid][pSpawnPoint] = 2;
		return 1;
	}
	return 1;
}
