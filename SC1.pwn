		}
	}
	else {
        foreach (new i : Player) if (!PlayerData[i][pDisableBC]) {
		    SendClientMessageEx(i, COLOR_LIGHTGREEN, "[Canal Estatal] Reportero %s: %s", ReturnName(playerid, 0), params);
		}
	}
	return 1;
}

