		    Dialog_Show(playerid, ViewCharges, DIALOG_STYLE_INPUT, "Ver cargos", "Error: Jugador inválido.\n\nPorfavor coloca el nombre o el id del juegador:", ">", "Atras");
		}
	}
	else cmd_mdc(playerid, "\1");
	return 1;
}

