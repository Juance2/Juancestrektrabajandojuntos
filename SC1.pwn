		}
        Dialog_Show(playerid, FindHouse, DIALOG_STYLE_INPUT, "Find House", "Error: No se ha encontrado resultados con \"%s\".\n\nPorfavor ingresa la dirección exacta de la casa:", "Buscar", "Cancelar", inputtext);
	}
	else cmd_gps(playerid, "\1");
	return 1;
}

