CMD:capo(playerid, params[])
{
	for (new i = 1; i != MAX_VEHICLES; i ++) if (IsValidVehicle(i) && IsPlayerNearHood(playerid, i))
	{
	    if (!IsDoorVehicle(i))
	        return SendErrorMessage(playerid, "Este vehículo no tiene capo.");

	    if (!GetHoodStatus(i))
		{
	        SetHoodStatus(i, true);

	        SendNearbyMessage(playerid, 30.0, COLOR_PURPLE, "* %s abre el capo del vehículo.", ReturnName(playerid, 0));
	        ShowPlayerFooter(playerid, "Capo ~g~abierto!");
		}
		else
		{
			SetHoodStatus(i, false);

	        SendNearbyMessage(playerid, 30.0, COLOR_PURPLE, "* %s cierra el capo del vehículo.", ReturnName(playerid, 0));
	        ShowPlayerFooter(playerid, "Capo ~g~cerrado!");
		}
	    return 1;
	}
	SendErrorMessage(playerid, "No estás cerca de ningún vehículo");
	return 1;
}

CMD:ventanas(playerid, params[])
