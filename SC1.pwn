CMD:hacerlider(playerid, params[])
{
	static
		userid,
		id;

    if (PlayerData[playerid][pAdmin] < 4)
	    return SendErrorMessage(playerid, "No estás autorizado para usar este comando");

	if (sscanf(params, "ud", userid, id))
	    return SendSyntaxMessage(playerid, "/hacerlider [id/nombre] [faccion] (Use -1 para sacar facción)");

	if (userid == INVALID_PLAYER_ID)
	    return SendErrorMessage(playerid, "La id especificada es inválida (Jugador desconectado)");

    if ((id < -1 || id >= MAX_FACTIONS) || (id != -1 && !FactionData[id][factionExists]))
	    return SendErrorMessage(playerid, "Has especificado una facción inválida.");

	if (id == -1)
	{
	    ResetFaction(userid);

	    SendServerMessage(playerid, "Has removido el liderazgo de la faccion a %s", ReturnName(userid, 0));
    	SendServerMessage(userid, "%s te ha removido el liderazgo de tu facción", ReturnName(playerid, 0));
	}
	else
	{
		SetFaction(userid, id);
		PlayerData[userid][pFactionRank] = FactionData[id][factionRanks];

		SendServerMessage(playerid, "Has convertido a %s colo líder de la facción \"%s\".", ReturnName(userid, 0), FactionData[id][factionName]);
    	SendServerMessage(userid, "%s te ha convertido en el líder de la facción \"%s\".", ReturnName(playerid, 0), FactionData[id][factionName]);
	}
    return 1;
}

CMD:darfaccion(playerid, params[])
{
	static
		userid,
		id;

    if (PlayerData[playerid][pAdmin] < 4)
	    return SendErrorMessage(playerid, "No estás autorizado para usar este comando");

	if (sscanf(params, "ud", userid, id))
	    return SendSyntaxMessage(playerid, "/darfaccion [id/nombre] [faccion] (Use -1 para sacar facción)");

	if (userid == INVALID_PLAYER_ID)
	    return SendErrorMessage(playerid, "La id especificada es inválida (Jugador desconectado)");

    if ((id < -1 || id >= MAX_FACTIONS) || (id != -1 && !FactionData[id][factionExists]))
	    return SendErrorMessage(playerid, "Has especificado una id inválida faction ID.");

	if (id == -1)
	{
	    ResetFaction(userid);

	    SendServerMessage(playerid, "Has removido a %s de su facción.", ReturnName(userid, 0));
