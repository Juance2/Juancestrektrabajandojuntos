        id = -1;

	new vehicleid = GetPlayerVehicleID(playerid);

	if (!IsEngineVehicle(vehicleid))
		return SendErrorMessage(playerid, "No estás en ningún vehículo.");

	if (GetPlayerState(playerid) != PLAYER_STATE_DRIVER)
	    return SendErrorMessage(playerid, "No eres el conductor del vehículo.");

	if (CoreVehicles[vehicleid][vehFuel] < 1)
	    return SendErrorMessage(playerid, "El vehículo se encuentra sin gasolina.");

	if (ReturnVehicleHealth(vehicleid) <= 300)
	    return SendErrorMessage(playerid, "El motor del vehículo se encuentra dañado.");

	switch (GetEngineStatus(vehicleid))
	{
	    case false:
	    {
			if(IsCamionero(vehicleid) && PlayerData[playerid][pJob] == JOB_COURIER) {
                ShowPlayerFooter(playerid, "Motor ~g~Encendido~w~");
                SendNearbyMessage(playerid, 30.0, COLOR_PURPLE, "* %s inserta la llave del vehículo y enciende el mismo.", ReturnName(playerid, 0));
				SetEngineStatus(vehicleid, true);
			}
			if(IsBasurero(vehicleid) && PlayerData[playerid][pJob] == JOB_GARBAGE) {
                ShowPlayerFooter(playerid, "Motor ~g~Encendido~w~");
                SendNearbyMessage(playerid, 30.0, COLOR_PURPLE, "* %s inserta la llave del vehículo y enciende el mismo.", ReturnName(playerid, 0));
				SetEngineStatus(vehicleid, true);
			}
            if ((id = Car_Nearest(playerid)) != -1)
	        {
                if (Car_IsOwner(playerid, id) || Car_IsOwnerR(playerid, id) || (PlayerData[playerid][pFaction] != -1 && CarData[id][carFaction] == GetFactionType(playerid)))
	            {
			        SetEngineStatus(vehicleid, true);
	                ShowPlayerFooter(playerid, "Motor ~g~Encendido~w~");
	                SendNearbyMessage(playerid, 30.0, COLOR_PURPLE, "* %s inserta la llave del vehículo y enciende el mismo.", ReturnName(playerid, 0));
                }
			    else return SendErrorMessage(playerid, "No posees las llaves del vehículo");
			}
		}
		case true:
		{
		    SetEngineStatus(vehicleid, false);
		    ShowPlayerFooter(playerid, "Motor ~r~apagado~w~");
		    SendNearbyMessage(playerid, 30.0, COLOR_PURPLE, "* %s apaga el motor del vehículo", ReturnName(playerid, 0));
		}
	}
	return 1;
}

