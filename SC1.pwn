			case 2:
    		{
    		    Dialog_Show(playerid, ViewCharges, DIALOG_STYLE_INPUT, "Ver cargos", "Coloca el nombre o la id del jugador:", ">", "Atras");
			}
	    }
	}
	return 1;
}

