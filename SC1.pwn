    if (userid == playerid)
	    return SendErrorMessage(playerid, "No puedes arrestarte a tí mismo.");

    if (!IsPlayerNearPlayer(playerid, userid, 5.0))
	    return SendErrorMessage(playerid, "Necesitas estar cerca del jugador.");

