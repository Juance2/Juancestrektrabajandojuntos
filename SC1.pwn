		    if (id != -1) {
		        PlayerData[playerid][pSelectedSlot] = id;

		        Dialog_Show(playerid, LocationInfo, DIALOG_STYLE_LIST, inputtext, "Ubicaciones\nBorrar ubicación", ">", "Atras");
			}
		}
	}
	else cmd_gps(playerid, "\1");
