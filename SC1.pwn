	if (!IsNewsVehicle(GetPlayerVehicleID(playerid)))
	    return SendErrorMessage(playerid, "No estás en una van de transmición.");

	if (!PlayerData[playerid][pBroadcast])
	{
	    PlayerData[playerid][pBroadcast] = true;

		SendNearbyMessage(playerid, 30.0, COLOR_PURPLE, "* %s ha comenzado una transmición.", ReturnName(playerid, 0));
	    SendServerMessage(playerid, "Has comenzado una nueva transmición (usa \"/bc [texto]\" para transmitir).");
	}
	else
	{
	    PlayerData[playerid][pBroadcast] = false;

		foreach (new i : Player) if (PlayerData[i][pNewsGuest] == playerid) {
		    PlayerData[i][pNewsGuest] = INVALID_PLAYER_ID;
		}
		SendNearbyMessage(playerid, 30.0, COLOR_PURPLE, "* %s ha parado la transmición.", ReturnName(playerid, 0));
	    SendServerMessage(playerid, "Has parado de transmitir.");
	}
	return 1;
}

